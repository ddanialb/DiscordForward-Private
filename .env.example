# Discord Message Forwarder Configuration
# Copy this file to .env and fill in your values

# Discord Token (required) - Your Discord account token
DISCORD_TOKEN=your_discord_token_here

# Channel IDs (required)
SOURCE_CHANNEL_ID=source_channel_id_here
DESTINATION_CHANNEL_ID=destination_channel_id_here

# Voice Channel (optional) - For 24/7 voice presence
VOICE_CHANNEL_ID=voice_channel_id_here

# Protected Users (optional) - Comma-separated list of user IDs to protect from voice disconnection
PROTECTED_USERS=941507108233416735